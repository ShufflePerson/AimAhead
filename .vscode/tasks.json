{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "MSBuild: Build AimAhead.vcxproj",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "AimAhead.vcxproj",
                "/property:Configuration=Release",
                "/property:Platform=x64"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}